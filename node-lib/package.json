{
  "name": "node-lib",
  "version": "0.1.0",
  "author": "Wang Guan <momocraft@gmail.com>",
  "private": true,
  "index": "build/lib/index",
  "bin": {
    "example-bin": "bin/example-bin"
  },
  "files": [
    "bin",
    "build",
    "!build/__test__/"
  ],
  "scripts": {
    "start": "node build/bin/example-bin",
    "build": "rm -rvf build && tsc",
    "local:install": "yarn build && yarn global add $PWD",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "gts lint",
    "lint:fix": "gts fix"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "debug": "^4.3.4",
    "fp-ts": "<3",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.5.5",
    "tslib": "^2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27",
    "@types/node": "^14",
    "eslint-plugin-import": "^2.26.0",
    "gts": "^3.1.0",
    "jest": "^28",
    "ts-jest": "^28",
    "typescript": "^4.6",
    "winston": "^3.7.2"
  },
  "resolutions": {
    "@types/node": "^14",
    "tslib": "^2"
  },
  "engines": {
    "node": ">=14"
  }
}
