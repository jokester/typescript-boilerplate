{
  "name": "browser-react",
  "version": "0.1.0",
  "author": "Wang Guan <momocraft@gmail.com>",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "analyze": "craco build && source-map-explorer 'build/static/js/*.js'",
    "test": "CI=true craco test",
    "test:watch": "craco test --watch",
    "test:coverage": "CI=true craco test --env=jsdom --coverage --watchAll=false --coverageReporters=json --coverageReporters=lcov --coverageReporters=text --coverageReporters=html",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "    eslint '**/*.@(ts|js|tsx|jsx)'",
    "lint:fix": "eslint '**/*.@(ts|js|tsx|jsx)' --fix"
  },
  "dependencies": {
    "preact": "^10.5.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.8",
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.1",
    "sass": "^1.32.2",
    "source-map-explorer": "^2.5.2",
    "ts-jest": "^26",
    "typescript": "^4.1",
    "web-vitals": "^1.0.1"
  },
  "resolutions": {}
}
