{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "formatter": "stylish",
  "jsRules": {},
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "quotemark": false,
    "max-line-length": [true, 160],
    "no-increment-decrement": false,
    "function-name": false,
    "no-this-assignment": false,
    "indent": [true, "spaces", 2]
  },
  "rulesDirectory": []
}
