{
  "name": "web-gatsby",
  "version": "0.1.0",
  "description": "Gatsby example site demonstrating how to use TypeScript with Gatsby",
  "private": "true",
  "license": "WTFPL",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "    eslint '**/*.@(ts|js|tsx|jsx)'",
    "lint:fix": "eslint '**/*.@(ts|js|tsx|jsx)' --fix"
  },
  "dependencies": {
    "gatsby": "^2.21.23",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tslib": "^1.11.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@types/jest": "^25",
    "@types/node": "^12",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "autoprefixer": "^9.7.6",
    "eslint": "7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "gatsby-cli": "^2.12.17",
    "gatsby-plugin-postcss": "^2.3.1",
    "gatsby-plugin-sass": "^2.3.1",
    "gatsby-plugin-typescript": "^2.4.2",
    "jest": "^25",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "ts-jest": "^25",
    "typescript": "^3.8"
  }
}
